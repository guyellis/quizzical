{
  "name": "quizzical",
  "version": "0.0.1",
  "description": "Test yourself on several quizzes",
  "main": "index.js",
  "scripts": {
    "build": "TARGET=build webpack",
    "start": "TARGET=dev webpack-dev-server --devtool eval-source --progress --colors --hot --inline --history-api-fallback",
    "prepublish": "npm run build",
    "pretest": "eslint -c .eslintrc --ext .js --ext .jsx src/",
    "test:jasmine": "jasmine JASMINE_CONFIG_PATH=test/server/support/jasmine.json",
    "test:karma": "./node_modules/karma/bin/karma start ./karma.conf.js",
    "test": "npm run test:jasmine && npm run test:karma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/quizzical.git"
  },
  "keywords": [
    "quiz"
  ],
  "author": "Guy Ellis <wildfiction@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guyellis/quizzical/issues"
  },
  "homepage": "https://github.com/guyellis/quizzical",
  "dependencies": {
    "alt": "~0.17.1",
    "bootstrap": "~3.3.5",
    "jquery": "~2.1.4",
    "lodash": "~3.10.0",
    "numeral": "~1.5.3",
    "react": "~0.13.3",
    "react-bootstrap": "~0.26.1",
    "react-router": "^0.13.4"
  },
  "devDependencies": {
    "babel-core": "^5.6.17",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.1",
    "css-loader": "^0.19.0",
    "eslint": "*",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "^1.6.0",
    "node-libs-browser": "^0.5.2",
    "react-hot-loader": "^1.2.7",
    "react-tools": "^0.13.3",
    "style-loader": "^0.12.3",
    "url-loader": "0.5.6",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1",
    "webpack-merge": "^0.2.0"
  }
}
