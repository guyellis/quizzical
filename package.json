{
  "name": "quizzical",
  "version": "0.0.1",
  "description": "Test yourself on several quizzes",
  "main": "index.js",
  "scripts": {
    "build": "TARGET=build webpack",
    "start": "TARGET=dev webpack-dev-server --devtool eval-source --progress --colors --hot --inline --history-api-fallback",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "lint": "eslint -c .eslintrc --ext .js --ext .jsx app/",
    "test:jasmine": "jasmine JASMINE_CONFIG_PATH=test/server/support/jasmine.json",
    "test:karma": "./node_modules/karma/bin/karma start ./karma.conf.js",
    "test": "npm run test:jasmine && npm run test:karma"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyellis/quizzical.git"
  },
  "keywords": [
    "quiz"
  ],
  "author": "Guy Ellis <wildfiction@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guyellis/quizzical/issues"
  },
  "homepage": "https://github.com/guyellis/quizzical",
  "dependencies": {
    "alt": "~0.18.6",
    "bootstrap": "~3.3.5",
    "jquery": "~3.2.1",
    "lodash": "~4.17.4",
    "numeral": "~2.0.6",
    "react": "~15.5.4",
    "react-bootstrap": "~0.31.0",
    "react-router": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "css-loader": "^0.28.0",
    "eslint": "*",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-libs-browser": "^2.0.0",
    "react-hot-loader": "^1.2.7",
    "react-tools": "^0.13.3",
    "style-loader": "^0.16.1",
    "url-loader": "0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
